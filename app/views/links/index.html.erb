<p id="notice"><%= notice %></p>

<h1>Links</h1>

<table>
  <thead>
    <tr>
      <th>Title</th>
      <th>Url</th>
      <th>Category</th>
      <th>User</th>
      <th>Slug</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @links.each do |l| %>
      <tr>
        <td><%= l.title %></td>
        <td><%= l.url %></td>
        <td><%= l.category_id %></td>
        <td><%= l.user_id %></td>
        <td><%= l.slug %></td>
        <td><%#= link_to 'Show', link_path(link) %></td>
        <td><%#= link_to 'Edit', edit_link_path(link) %></td>
        <td><%#= link_to 'Destroy', link, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Link', new_link_path %>
